<div id="<%= dom_id message %>">
  <p>
    <strong>Title:</strong>
    <%= message.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= message.description %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= message.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= message.end_time %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= message.status %>
  </p>

</div>
